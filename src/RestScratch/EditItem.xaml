        <Label Content="Value" Height="28" HorizontalAlignment="Left" Margin="12,46,0,0" Name="label2" VerticalAlignment="Top" Width="50" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="68,14,0,0" Name="tbKey" VerticalAlignment="Top" Width="198" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="68,48,0,0" Name="tbValue" VerticalAlignment="Top" Width="198" />
